package model.grains;

/**
 * 
**/

public enum GrainType
{
	// Grain Categories
	Cylindrical,
	Star;
	
	
	
	/**
	 * toString()
	 * 
	 * Purpose: Returns the string representation of the grain type.
	 * 
	 * Parameters:
	 * 
	 * Returns: String. The string representation of the grain type.
	**/
	
	public String toString ()
	{
		return this.name();
	} // toString()
	
} // enum GrainType
