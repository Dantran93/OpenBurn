package controller;

import view.grain.input.AddGrainWindow;
import view.grain.input.GrainInputView;

/**
 * 
**/

public class GrainHandler
{
	public static GrainHandler instance = null;
	
	
	
	
	
	
	private GrainHandler () {}
	
	
	
	public GrainHandler getInstance ()
	{
		if (instance == null)
			return new GrainHandler();
		else
			return instance;
	}
	
	
}
